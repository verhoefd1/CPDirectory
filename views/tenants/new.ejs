<%include ../partials/header%>
	<div>
        <h1 style="text-align: center;">Add a new Tenant</h1>
        <div style="width: 30%; margin:45px auto;">
            <!--sets form up to send a post back through the server under /campgrounds-->
            <form action="/tenants" method="POST">
                <div class="form-group">
                    <input class="form-control" type="text"  name="name" placeholder="Tenant Name">
                </div>
                <div class="form-group">
                    <input class="form-control" type="number" name="suite" placeholder="Suite Number">
                </div>
                <div class="form-group">
                    <select class="form-control" type="number" name="column" placeholder="Column 1 - 3">
                    	<option value="1">Column 1</option>
                    	<option value="2">Column 2</option>
                    	<option value="3">Column 3</option>
                    </select>
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="subname1" placeholder="Individual user to be listed. Input how you'd like it displayed.">
                    <input class="form-control" type="text" name="subname2" placeholder="Individual user to be listed. Input how you'd like it displayed.">
                    <input class="form-control" type="text" name="subname3" placeholder="Individual user to be listed. Input how you'd like it displayed.">
                    <input class="form-control" type="text" name="subname4" placeholder="Individual user to be listed. Input how you'd like it displayed.">
                    <input class="form-control" type="text" name="subname5" placeholder="Individual user to be listed. Input how you'd like it displayed.">
                    <input class="form-control" type="text" name="subname6" placeholder="Individual user to be listed. Input how you'd like it displayed.">
                </div> 
                <button type="Submit" class="btn btn-primary">Submit</button>          
            </form>
        </div>
    </div>
<%include ../partials/footer%>